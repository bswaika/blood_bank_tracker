<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_alignParentTop="true"
    android:layout_alignParentStart="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="bswaika96.gmail.com.mapdemo.MapsActivity" />

    <Button
        android:id="@+id/profileButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="41dp"
        android:layout_marginEnd="16dp"
        android:layout_toStartOf="@+id/appointButton"
        android:text="PROFILE" />

    <Button
        android:id="@+id/appointButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/profileButton"
        android:layout_centerHorizontal="true"
        android:text="APPOINTMENTS" />

    <Button
        android:id="@+id/logoutButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/appointButton"
        android:layout_alignBottom="@+id/appointButton"
        android:layout_marginStart="16dp"
        android:layout_toEndOf="@+id/appointButton"
        android:text="LOGOUT" />


</RelativeLayout>
